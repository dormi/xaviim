@import url(http://fonts.googleapis.com/css?family=Open+Sans:400italic,400,700);

@charset "UTF-8";
 
@font-face {
  font-family: "dormifumi";
  src: url("<%= asset_path('icofonts.eot') %>");
  src: url("<%= asset_path('dormifumi.eot?#iefix') %>") format("embedded-opentype"),
       url("<%= asset_path('dormifumi.ttf') %>") format("truetype"),
       url("<%= asset_path('dormifumi.svg#dormifumi') %>") format("svg"),
       url("<%= asset_path('dormifumi.woff') %>") format("woff");
  font-weight: normal;
  font-style: normal;
}
 
[data-icon]:before {
  font-family: "dormifumi" !important;
  content: attr(data-icon);
  font-style: normal !important;
  font-weight: normal !important;
  font-variant: normal !important;
  text-transform: none !important;
  speak: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
 
[class^="icon-"]:before,
[class*=" icon-"]:before {
  font-family: "dormifumi" !important;
  font-style: normal !important;
  font-weight: normal !important;
  font-variant: normal !important;
  text-transform: none !important;
  speak: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
} 
 
.icon-github:before {
   content: "g"; 
} 
 
.icon-twitter:before {
   content: "t"; 
} 
 
.icon-blog:before {
   content: "b"; 
}